#tag Events Button1
	#tag Event
		Sub Pressed()
		  //MyDB.ExecuteSQL("CREATE TABLE tblPlaylists (playlistID INTEGER PRIMARY KEY AUTOINCREMENT, user_ID INTEGER, Type  INTEGER, playlistName TEXT, playlistDescription TEXT, playlistMeta TEXT)")
		  Var sql, type as String
		  if playlistID.Text="" then
		    
		    if playlistID.Text="" then //Add new
		      
		      if privPopup.SelectedRowIndex=0 then
		        type="1"
		      else
		        type="10"
		      end if
		      
		      sql="INSERT INTO tblPlaylists (user_ID, Type, PlaylistName) VALUES(?,?,?)"
		      
		      Try
		        MyDB.ExecuteSQL(sql, session.myUserID, type, playlistName.text)
		      Catch error As DatabaseException
		        MessageBox( "DB Error " + error.Message)
		        Return
		      End Try
		      
		    else //edit existing
		      
		    end if
		  else
		    
		    if privPopup.SelectedRowIndex=0 then
		      type="1"
		    else
		      type="10"
		    end if
		    
		    sql = "UPDATE tblPlaylists SET playlistName = ?, Type = ? WHERE playlistID = ?"
		    Try
		      MyDB.ExecuteSQL(sql, playlistName.Text, type, playlistID.Text)
		    Catch error As DatabaseException
		      MessageBox( "DB Error " + error.Message)
		      Return
		    End Try
		    
		  end if
		  
		  
		  ListPlayLists
		  Session.kastalPage.MyMusicContainer.ListLibraries
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events playlistListBox
	#tag Event
		Sub SelectionChanged(rows() As Integer)
		  //MyDB.ExecuteSQL("CREATE TABLE tblPlaylists (playlistID INTEGER PRIMARY KEY AUTOINCREMENT, user_ID INTEGER, Type  INTEGER, playlistName TEXT, playlistDescription TEXT, playlistMeta TEXT)")
		  
		  if me.SelectedRowIndex>=0 then
		    Var rs As RowSet
		    Try
		      rs = MyDB.SelectSQL("SELECT * FROM tblPlaylists WHERE playlistID = ?", me.RowTagAt(me.SelectedRowIndex).StringValue)
		    end Try
		    For Each row As DatabaseRow In rs
		      playlistID.Text=row.Column("playlistID").StringValue
		      playlistName.Text=row.Column("playlistName").StringValue
		      if row.Column("Type").StringValue="1" then
		        privPopup.SelectedRowIndex=0
		      else
		        privPopup.SelectedRowIndex=1
		      end If
		    Next
		  else
		    playlistID.Text=""
		    playlistName.Text=""
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents

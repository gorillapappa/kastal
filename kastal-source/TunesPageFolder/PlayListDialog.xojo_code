#tag WindowCode
	#tag Event
		Sub Shown()
		  ListPlayLists
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub ListPlayLists()
		  Var rs as RowSet
		  playlistListBox.RemoveAllRows
		  rs = MyDB.SelectSQL("SELECT * FROM tblPlaylists WHERE user_ID = "+Session.myUserID.ToString+" OR Type > 9")
		  For Each row As DatabaseRow In rs
		    playlistListBox.AddRow(row.Column("playlistName").StringValue)
		    playlistListBox.RowTagAt(playlistListBox.LastAddedRowIndex) = row.Column("playlistID").StringValue
		    if playlistID.Text=row.Column("playlistID").StringValue then
		      playlistListBox.SelectedRowIndex=playlistListBox.LastAddedRowIndex
		    end if
		  Next
		End Sub
	#tag EndMethod


#tag EndWindowCode


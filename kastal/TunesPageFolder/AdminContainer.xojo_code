	#tag Method, Flags = &h0
		Sub listUsers()
		  
		  Var rs As RowSet
		  UsersListbox.RemoveAllRows
		  'Button5.Enabled=false
		  Try
		    rs = MyDB.SelectSQL("SELECT * FROM tblUsers")
		    For Each row As DatabaseRow In rs
		      UsersListbox.AddRow(row.Column("userID").StringValue, row.Column("State").StringValue, row.Column("Email").StringValue, row.Column("FirstName").StringValue, row.Column("LastName").StringValue)
		      UsersListbox.RowTagAt(UsersListbox.LastAddedRowIndex)=row.Column("userID").IntegerValue
		    Next
		    rs.Close
		  Catch error As DatabaseException
		    MessageBox("Error: " + error.Message)
		  End Try
		End Sub
	#tag EndMethod



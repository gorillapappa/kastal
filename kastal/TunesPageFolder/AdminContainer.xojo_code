#tag Events UsersListbox
	#tag Event
		Sub Opening()
		  listUsers
		End Sub
	#tag EndEvent
	#tag Event
		Sub SelectionChanged(rows() As Integer)
		  myStatePopup.Enabled=false
		  
		  if me.SelectedRowIndex>=0 then
		    EmailFld.Enabled=false
		    PWFld.enabled=false
		    SaveUserBtn.Enabled=true
		    DeleteUsrBtn.enabled=true
		    
		    Var myState, i As Integer
		    
		    
		    uID.Text=me.RowTagAt(UsersListbox.SelectedRowIndex).StringValue
		    myState=me.CellTextAt(me.SelectedRowIndex, 1).ToInteger
		    
		    for i=0 to myStatePopup.RowCount-1
		      if myStatePopup.RowTagAt(i) = myState then
		        myStatePopup.SelectedRowIndex=i
		        myStatePopup.Enabled=True
		      end if
		    next
		    
		    Var rs As RowSet
		    Try
		      rs = MyDB.SelectSQL("SELECT * FROM tblUsers WHERE userID = ?", UsersListbox.RowTagAt(UsersListbox.SelectedRowIndex).IntegerValue)
		      For Each row As DatabaseRow In rs
		        UserData.Text=row.Column("FirstName").StringValue+" "+row.Column("LastName").StringValue+EndOfLine+row.Column("Adr1").StringValue+EndOfLine+row.Column("Adr2").StringValue+EndOfLine+row.Column("PostalCode").StringValue+" "+row.Column("City").StringValue+EndOfLine+row.Column("Country").StringValue+EndOfLine+EndOfLine+"Email: "+row.Column("Email").StringValue+EndOfLine+"Tel: "+row.Column("Phone").StringValue
		      Next
		      rs.Close
		    Catch error As DatabaseException
		      MessageBox("Error: " + error.Message)
		    End Try
		    
		    
		  else
		    DeleteUsrBtn.enabled=false
		    SaveUserBtn.Enabled=false
		    myStatePopup.SelectedRowIndex=-1
		    EmailFld.Enabled=false
		    PWFld.enabled=false
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SaveUserBtn
	#tag Event
		Sub Pressed()
		  Var sql as String
		  Var d As DateTime = DateTime.Now
		  
		  if uID.Text="" then //Add new user
		    
		    //Here we store the innitialuser in the DB
		    sql = "INSERT INTO tblUsers (Email, creationDate, State, Password, FirstName, LastName) VALUES(?, ?, ?, ?, ?, ?)"
		    Try
		      MyDB.ExecuteSQL(sql, EmailFld.Text, d.SQLDateTime, myStatePopup.RowTagAt(myStatePopup.SelectedRowIndex).IntegerValue, PasswordHash(EmailFld.Text+PWFld.text,512), "User", "Name")
		    Catch error As DatabaseException
		      MessageBox( "DB Error " + error.Message)
		      Return
		    End Try
		    
		  else //Save current user
		    
		    sql = "UPDATE tblUsers SET State = ? WHERE userID = ?"
		    Try
		      MyDB.ExecuteSQL(sql, myStatePopup.RowTagAt(myStatePopup.SelectedRowIndex).IntegerValue, uid.Text)
		    Catch error As DatabaseException
		      MessageBox( "DB Error " + error.Message)
		    End Try
		    
		  end if
		  
		  listUsers
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Button6
	#tag Event
		Sub Pressed()
		  uID.Text=""
		  myStatePopup.SelectedRowIndex=2
		  myStatePopup.Enabled=true
		  EmailFld.Enabled=true
		  PWFld.enabled=true
		  SaveUserBtn.enabled=True
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events DeleteUsrBtn
	#tag Event
		Sub Pressed()
		  Var sql as String
		  if uID.Text<>"" then //Delete current user
		    
		    sql = "DELETE FROM tblUsers WHERE userID = ?"
		    Try
		      MyDB.ExecuteSQL(sql, uid.Text)
		    Catch error As DatabaseException
		      MessageBox( "DB Error " + error.Message)
		    End Try
		    
		  end if
		  
		  me.Enabled=false
		  SaveUserBtn.Enabled=false
		  myStatePopup.SelectedRowIndex=-1
		  EmailFld.Enabled=false
		  PWFld.enabled=false
		  
		  listUsers
		End Sub

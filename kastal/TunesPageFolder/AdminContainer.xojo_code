	#tag Event
		Sub Run()
		  if MyScan4TunesThread<>nil then
		    if MyScan4TunesThread.ThreadState=Thread.ThreadStates.NotRunning then
		      MusicScannerProgress.Visible=false
		    else
		      MusicScannerProgress.Visible=true
		    end if
		  end if
		  
		  
		  if MyScan4MoviesThread<>nil then
		    if MyScan4MoviesThread.ThreadState=Thread.ThreadStates.NotRunning then
		      MovieScannerProgress.Visible=false
		    else
		      MovieScannerProgress.Visible=true
		    end if
		  end if
		End Sub
